[
	{
		"bindings": {
			"ctrl-p": "menu::SelectNext",
			"ctrl-n": "menu::SelectPrevious"
		}
	},
	{
		"context": "VimControl && !menu",
		"bindings": {
			"[ d": "editor::GoToDiagnostic",
			"] d": "editor::GoToPreviousDiagnostic"
		}
	},
	{
		"context": "Editor && vim_mode == normal && !VimWaiting && !menu",
		"bindings": {
			"ctrl-e": "project_panel::ToggleFocus",
			"ctrl-[": "pane::DeploySearch",
			"ctrl-p": "file_finder::Toggle",
			"ctrl-enter": "editor::ToggleCodeActions",
			"ctrl-space r": "editor::Rename",
			"ctrl-h": "workspace::ActivatePaneLeft",
			"ctrl-l": "workspace::ActivatePaneRight",
			"ctrl-k": "workspace::ActivatePaneUp",
			"ctrl-j": "workspace::ActivatePaneDown"
		}
	},
	{
		"context": "ProjectPanel && not_editing",
		"bindings": {
			"ctrl-e": "workspace::ToggleRightDock",
			"o": "project_panel::NewFile",
			"x": "project_panel::Cut",
			"y y": "project_panel::Copy",
			"p": "project_panel::Paste",
			"shift-r": "project_panel::Rename",
			"shift-d": [
				"project_panel::Trash",
				{
					"skip_prompt": true
				}
			],
			"ctrl-h": "workspace::ActivatePaneLeft",
			"ctrl-l": "workspace::ActivatePaneRight",
			"ctrl-k": "workspace::ActivatePaneUp",
			"ctrl-j": "workspace::ActivatePaneDown"
		}
	},
	{
		"context": "FileFinder > Picker > Editor",
		"bindings": {
			"ctrl-x": "pane::SplitDown",
			"ctrl-v": "pane::SplitRight"
		}
	}
]
