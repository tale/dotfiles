Include ~/.ssh/private.config
Include ~/.orbstack/ssh/config

Host *
	IdentityAgent ~/.1password/agent.sock
	ForwardAgent yes
	Compression yes
	ServerAliveInterval 0
	ServerAliveCountMax 3
	HashKnownHosts no
	UserKnownHostsFile ~/.ssh/known_hosts
	ControlMaster auto
	ControlPath /tmp/ssh-%C
	ControlPersist no

